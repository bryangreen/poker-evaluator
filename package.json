{
  "name": "poker-evaluator",
  "version": "1.0.0",
  "description": "A poker hand evaluator",
  "scripts": {
    "typings-install": "typings install",
    "postinstall": "npm run typings-install",
    "build": "webpack --inline --colors --progress --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "server": "webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 3001  --content-base src",
    "server-debug": "webpack-dev-server --colors --progress --display-error-details --display-cached --port 3001  --content-base src",
    "start": "npm run server",
    "test": "karma start",
    "production-build": "set env NODE_ENV=production && webpack -p --progress --colors --display-error-details --display-cached"
  },
  "contributors": [
    "Bryan Green <mail@bryangreen.me>",
    "Rob Wormald <robwormald@gmail.com>",
    "PatrickJS <github@gdi2290.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "angular-cli-ghpages": "^0.1.7",
    "angular2-template-loader": "^0.4.0",
    "awesome-typescript-loader": "^1.1.1",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "karma": "^1.2.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sinon": "^1.0.5",
    "karma-typescript-preprocessor2": "^1.2.1",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "ng2-bootstrap": "^1.0.24",
    "phantomjs-prebuilt": "^2.1.12",
    "raw-loader": "^0.5.1",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.4",
    "typescript": "~1.8.9",
    "typings": "~1.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0",
    "webpack-merge": "^0.8.4"
  },
  "dependencies": {
    "@angular/forms": "^0.2.0",
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.4.0",
    "ie-shim": "^0.1.0",
    "lodash": "^4.0.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12"
  },
  "keywords": [
    "poker",
    "poker hand"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bryangreen/poker-evaluator.git"
  },
  "bugs": {
